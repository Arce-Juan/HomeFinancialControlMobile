<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HomeFinancialControl.Presentation.Views.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Conceptos"
                Clicked="ConceptButtonClicked"
                HeightRequest="80"
                WidthRequest="80">
                <Button.ImageSource>
                    <FileImageSource File="inmigracion.png" />
                </Button.ImageSource>
            </Button>
            
            <Button Text="Movimientos"
                Clicked="MovementButtonClicked"
                HeightRequest="80"
                WidthRequest="80">
                <Button.ImageSource>
                    <FileImageSource File="inmigracion.png" />
                </Button.ImageSource>
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>